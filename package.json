{
  "name": "postcss-each-decl",
  "version": "1.0.0",
  "description": "PostCSS helper method to shallowly iterate over each declaration.",
  "main": "dist/lib/index.js",
  "scripts": {
    "prepublish": "gulp && gulp copy && npm run babelify",
    "babelify": "babel build/lib --out-dir dist/lib",
    "test": "gulp && npm run cover && npm run check-coverage",
    "cover": "isparta cover node_modules/mocha/bin/_mocha -i build/lib/index.js",
    "check-coverage": "istanbul check-coverage",
    "watch": "sh scripts/watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedmao/postcss-each-decl.git"
  },
  "keywords": [
    "postcss",
    "helpers",
    "each",
    "decl",
    "declaration"
  ],
  "author": "Jed Mao <jedmao@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedmao/postcss-each-decl/issues"
  },
  "homepage": "https://github.com/jedmao/postcss-each-decl#readme",
  "dependencies": {
    "postcss": "^5.0.5"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.2",
    "chai": "^3.2.0",
    "del": "^2.0.2",
    "eslint": "^1.4.1",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-plumber": "^1.0.1",
    "gulp-tslint": "^3.2.0",
    "gulp-typescript": "^2.8.3",
    "isparta": "^3.0.4",
    "istanbul": "^0.3.20",
    "mocha": "^2.3.2"
  }
}
